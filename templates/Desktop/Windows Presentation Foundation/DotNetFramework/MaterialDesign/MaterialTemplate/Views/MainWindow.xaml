<controls:MetroWindow x:Class="MaterialTemplate.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mvvm="http://prismlibrary.com/"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:designTimes="clr-namespace:MaterialTemplate.DesignTimes"
        mvvm:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
                      d:DataContext="{d:DesignInstance Type={x:Type designTimes:MainWindowViewModel}, IsDesignTimeCreatable=True}"
        Title="{Binding Title}" Height="350" Width="525">
    <Grid />
</controls:MetroWindow>

